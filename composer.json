{
    "name": "appneta/traceview-bundle",
    "type": "symfony-bundle",
    "description": "Additional integration with AppNeta TraceView for Symfony2, including Twig support.",
    "keywords": ["symfony", "twig", "traceview", "appneta", "performance", "monitoring", "apm"],
    "homepage": "https://github.com/appneta/AppNetaTraceViewBundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "James Meickle",
            "email":    "jmeickle@appneta.com",
            "homepage": "https://github.com/Eronarn",
            "role":     "Maintainer"
        },
        {
            "name":     "AppNeta",
            "email":    "traceviewsupport@appneta.com",
            "homepage": "https://github.com/appneta",
            "role":     "Sponsor"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.1"
    },
    "suggest": {
        "ext-oboe": "`AppNeta/TraceViewBundle` will not report data unless the `php-oboe` extension is enabled at v1.4.4 or higher."
    },
    "autoload": {
        "psr-0": { "AppNeta\\TraceViewBundle": "" },
        "files": ["Resources/stubs/api.php"]
    },
    "target-dir": "AppNeta/TraceViewBundle"
}
